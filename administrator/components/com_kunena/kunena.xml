	<versionname>Omega</versionname>
	<creationDate>2012-01-31</creationDate>
	<build>5215</build>
	<author>Kunena Team</author>
	<authorEmail>kunena@kunena.org</authorEmail>
	<authorUrl>http://www.kunena.org</authorUrl>
	<copyright>(C) 2008 - 2012 Kunena Team. All rights reserved.</copyright>
	<license>GNU/GPL</license>
	<description>Kunena Forum</description>
	<scriptfile>install.script.php</scriptfile>
	<files>
		<filename>kunena.php</filename>
		<filename>router.php</filename>
		<filename>CHANGELOG.php</filename>
		<filename>COPYRIGHT.php</filename>
	</files>
	<administration>
		<menu task="cpanel" img="components/com_kunena/media/icons/kunena-favicon.png">COM_KUNENA</menu>
		<submenu>
			<menu task="showconfig" img="components/com_kunena/media/icons/config-favicon.png">COM_KUNENA_CONFIGURATION</menu>
