			<menu task="showTemplates" img="components/com_kunena/media/icons/templates-favicon.png">COM_KUNENA_TEMPLATE_MANAGER</menu>
			<menu task="ranks" img="components/com_kunena/media/icons/ranks-favicon.png">COM_KUNENA_RANK_MANAGER</menu>
			<menu task="showtrashview" img="components/com_kunena/media/icons/trash-favicon.png">COM_KUNENA_TRASH_MANAGER</menu>
		</submenu>
		<files>
			<filename>api.php</filename>
			<filename>admin.kunena.php</filename>
			<filename>install.script.php</filename>
			<folder>install</folder>
			<folder>archive</folder>
		</files>
	</administration>
	<updateservers>
		<server type="collection" priority="1" name="Kunena 1.7 Update Site">http://update.kunena.org/1.7</server>
